\n    ;  height: 80px;\r\n      width: 100%;\r\n    }\r\n    label.logo{\r\n      color: white;\r\n      font-size: 25px;\r\n      line-height: 80px;\r\n      padding: 0 100px;\r\n      font-weight: bold;\r\n    }\r\n    nav ul{\r\n      margin: 0;\r\n      padding: 0;\r\n      float: right;\r\n      margin-right: 2px;\r\n    }\r\n    nav ul li{\r\n      display: inline-block;\r\n      line-height: 80px;\r\n      margin: 0 5px;\r\n    }\r\n    nav ul li a{\r\n      color: rgb(255, 255, 255);\r\n      font-size: 25px;\r\n      padding: 7px 13px;\r\n      border-radius: 3px;\r\n      text-decoration: none;\r\n    }\r\n    a.active,a:hover{\r\n      background: #a9a244;\r\n      box-shadow: 0 0 10px #ffca28, 0 0 20px #ffca28, 0 0 30px #ffca28; \r\n\r\n      transition: .5s;\r\n    }\r\n    .checkbtn{\r\n      font-size: 30px;\r\n      color: white;\r\n      float: right;\r\n      line-height: 80px;\r\n      margin-right: 40px;\r\n      cursor: pointer;\r\n      display: none;\r\n    }\r\n    #check{\r\n      display: none;\r\n    }\r\n    @media (max-width: 952px){\r\n      label.logo{\r\n        font-size: 30px;\r\n        padding-left: 50px;\r\n      }\r\n      nav ul li a{\r\n        font-size: 16px;\r\n      }\r\n    }\r\n    @media (max-width: 858px){\r\n      .checkbtn{\r\n        display: block;\r\n      }\r\n      ul{\r\n        position: fixed;\r\n        width: 100%;\r\n        height: 100vh;\r\n        background: #d6930e;\r\n        top: 80px;\r\n        left: -100%;\r\n        text-align: center;\r\n        transition: all .5s;\r\n        z-index: 9999;\r\n      }\r\n      nav ul li{\r\n        display: block;\r\n        margin: 50px 0;\r\n        line-height: 30px;\r\n      }\r\n      nav ul li a{\r\n        font-size: 20px;\r\n      }\r\n      a:hover,a.active{\r\n        background: none;\r\n        color: #0082e6;\r\n      }\r\n      #check:checked ~ ul{\r\n        left: 0;\r\n      }\r\n    }\r\n    ","sass\\components\\_images.scss":" // image\r\n .obimage {\r\n    max-width: 100%;\r\n    height: auto; \r\n    border-radius: 2%;\r\n  }\r\n  ","sass\\components\\_button.scss":"//button\r\n\r\n\r\n\r\n\r\n#restart-game {\r\n    background-color: transparent;\r\n    color: white;\r\n    border: 2px solid white;\r\n    padding: 10px 20px;\r\n    border-radius: 5px;\r\n    font-size: 16px;\r\n    cursor: pointer;\r\n    transition: background-color 0.3s, color 0.3s; \r\n  }\r\n  \r\n  #restart-game:hover {\r\n    background-color: #a9a244; \r\n    color: rgb(0, 0, 0); \r\n    box-shadow: 0 0 10px #ffca28, 0 0 20px #ffca28, 0 0 30px #ffca28; \r\n\r\n  }\r\n\r\n  \r\n  //not a button but still\r\n  #tries-left {\r\n    position: absolute;\r\n    top: 500px; /* Adjust top position */\r\n    left: 50%; /* Adjust left position */\r\n    transform: translateX(-50%);\r\n    font-size: 18px;\r\n    color: #ff0000;\r\n    border: 2px solid #ff0000;\r\n    border-radius: 8px;\r\n    padding: 8px;\r\n  }\r\n  \r\n  \r\n  ","sass\\components\\_gamebox.scss":".win-message,\r\n.lose-message,\r\n.message {\r\n  color: rgb(255, 255, 255); \r\n  position: fixed; \r\n  top: 50%; \r\n  left: 50%; \r\n  transform: translate(-50%, -50%); \r\n  animation: pop 0.5s ease forwards; \r\n  padding: 10px 20px; \r\n  border-radius: 8px; \r\n}\r\n\r\n.win-message::before,\r\n.lose-message::before,\r\n.message::before {\r\n  content: '';\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  background-color: rgb(255, 0, 0);\r\n  border-radius: inherit; \r\n  z-index: -1;\r\n}\r\n\r\n\r\n\r\n\r\n#game-container {\r\n  position: relative;\r\n  .game-info {\r\n    position: absolute;\r\n    top: -50px;\r\n    left: 50%;\r\n    transform: translate(-50%, -50%); \r\n    display: flex;\r\n    justify-content: space-between;\r\n    width: 100%;\r\n    max-width: 500px;\r\n    margin-bottom: 20px;\r\n  }\r\n\r\n  #game-board {\r\n    display: grid;\r\n    grid-template-columns: repeat(5, 1fr);\r\n    max-width: 400px;\r\n    margin: auto;\r\n  }\r\n\r\n  .winner-message {\r\n    display: none;\r\n    font-size: 3rem;\r\n    font-weight: bold;\r\n    text-align: center;\r\n    margin-top: 20px;\r\n\r\n    &.hidden {\r\n      display: none;\r\n    }\r\n  }\r\n}\r\n\r\n.tile {\r\n  background-color: #fff;\r\n  border: 6px solid #000;\r\n  border-radius: 18px;\r\n  padding: 20px;\r\n  font-size: 30px;\r\n  text-align: center;\r\n  vertical-align: middle;\r\n  cursor: pointer;\r\n  transition: box-shadow 0.3s; \r\n}\r\n\r\n.tile:hover {\r\n  box-shadow: 0 0 10px #ffca28, 0 0 20px #ffca28, 0 0 30px #ffca28; \r\n}\r\n\r\n\r\n.blurred {\r\n  filter: blur(10px);\r\n  pointer-events: none;\r\n}\r\n\r\n.tile {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  width: 100px; \r\n  height: 100px; \r\n  background-size: contain; \r\n  background-repeat: no-repeat;\r\n\r\n  @media (max-width: 768px) {\r\n    width: 60px; \r\n    height: 60px; \r\n    font-size: 20px; \r\n  }\r\n}","sass\\pages\\_home.scss":"\r\n* {\r\n  box-sizing: border-box;\r\n}\r\n\r\nbody{\r\n\r\n  justify-content: center;\r\n  align-items: center;\r\n  font-family: Arial, sans-serif;\r\nbackground-color: #060606;\r\n\r\n}\r\nhtml {\r\n  height: 100%;\r\n}\r\n\r\n\r\n\r\n\r\n//heading\r\n.heading {\r\n  text-align: center; \r\n}\r\n\r\n.jackpot {\r\n  font-size: 36px; \r\n  margin: 0; \r\n  color: #ffca28; \r\n  animation: glow 2s infinite alternate; \r\n}\r\n\r\n@keyframes glow {\r\n  from {\r\n    text-shadow: 0 0 10px #ffca28, 0 0 20px #ffca28, 0 0 30px #ffca28; /* Define glow effect */\r\n  }\r\n  to {\r\n    text-shadow: 0 0 20px #ffca28, 0 0 30px #ffca28, 0 0 40px #ffca28; /* Define glow effect */\r\n  }\r\n}\r\n\r\n@media (max-width: 768px) {\r\n  .jackpot {\r\n    font-size: 24px; /* Decrease font size for mobile */\r\n  }\r\n}\r\n\r\n\r\n\r\n// rules\r\n\r\n.rules {\r\n  text-align: center;\r\n  background-color: #ffffff; \r\n  padding: 20px; \r\n}\r\n\r\n.game-rule {\r\n  color: 